<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!-- Handles getting the keystore password as well as allowing the user to 
	specify a new certificate, whether it is for the client or server. -->


<AnchorPane minHeight="250.0" minWidth="300.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65">
	<children>
		<Label layoutX="87.0" layoutY="14.0" text="Keystore Access">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<Separator layoutY="40.0" prefHeight="0.0" prefWidth="300.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
		<Label layoutX="14.0" layoutY="43.0"
			text="Please enter the password to unlock the keystore:"
			AnchorPane.leftAnchor="14.0" />
		<PasswordField layoutX="13.0" layoutY="60.0" prefHeight="25.0"
			prefWidth="275.0" AnchorPane.leftAnchor="14.0"
			AnchorPane.rightAnchor="14.0" />
		<Button layoutX="101.0" layoutY="92.0" mnemonicParsing="false"
			text="Unlock keystore" />
		<Separator layoutX="2.0" layoutY="124.0" prefHeight="0.0"
			prefWidth="300.0" AnchorPane.bottomAnchor="123.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="125.0" />
		<Accordion layoutX="3.0" layoutY="126.0" prefHeight="121.0"
			prefWidth="303.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="130.0">
			<panes>
				<TitledPane animated="false" expanded="false"
					text="Generate Certificate" />
			</panes>
		</Accordion>
	</children>
</AnchorPane>
