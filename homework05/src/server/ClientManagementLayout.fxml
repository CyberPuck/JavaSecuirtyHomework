<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!-- Handles the client certificate information: displaying to the user, 
	allowing for clearance level updating, and revoking the client's certificate. -->


<AnchorPane prefHeight="100.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65">
   <children>
      <Label fx:id="userNameLabel" layoutX="14.0" layoutY="6.0" text="User: &lt;name&gt;" />
      <Label layoutX="14.0" layoutY="23.0" text="Clearance Level:" />
      <ComboBox fx:id="clearanceCombo" layoutX="109.0" layoutY="19.0" prefHeight="25.0" prefWidth="46.0" />
      <Button layoutX="14.0" layoutY="50.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="223.0" text="Revoke Certificate">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
